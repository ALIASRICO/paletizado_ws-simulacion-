      </geometry>
    </collision>
  </link>

  <joint name="joint6" type="revolute">
    <origin rpy="-1.5708 0 0" xyz="0 0.1365 0"/>
    <parent link="Link5"/>
    <child link="Link6"/>
    <axis xyz="0 0 1"/>
    <limit effort="50.0" lower="-6.28" upper="6.28" velocity="3.14"/>
    <dynamics damping="2.0" friction="0.1"/>
  </joint>

</robot>