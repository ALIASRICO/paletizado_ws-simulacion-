    <parent link="Link2"/>
    <child link="Link3"/>
    <axis xyz="0 0 1"/>
    <limit effort="150.0" lower="-2.9" upper="2.9" velocity="3.14"/>
    <dynamics damping="8.0" friction="0.4"/>
  </joint>

  <!-- ============================================================ -->
  <!-- LINK 4                                                        -->
  <!-- ============================================================ -->
  <link name="Link4">
    <inertial>
      <origin rpy="0 0 0" xyz="0.0 -0.07573 0.0"/>
      <mass value="3.0"/>
      <inertia ixx="0.01" ixy="0.0" ixz="0.0"
                           iyy="0.01" iyz="0.0"
                                       izz="0.005"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://cra_description/meshes/cr20/Link4.STL"/>
      </geometry>
      <material name="cr20_white"/>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://cra_description/meshes/cr20/Link4.STL"/>
