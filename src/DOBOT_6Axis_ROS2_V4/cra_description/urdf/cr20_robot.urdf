<robot name="cr20_robot">

  <!-- ============================================================ -->
  <!-- MATERIALES                                                    -->
  <!-- ============================================================ -->
  <material name="cr20_white">
    <color rgba="0.792 0.820 0.933 1.0"/>
  </material>

  <material name="cr20_dark">
    <color rgba="0.2 0.2 0.2 1.0"/>
  </material>

  <!-- ============================================================ -->
  <!-- WORLD LINK                                                    -->
  <!-- ============================================================ -->
  <link name="world"/>

  <!-- ============================================================ -->
  <!-- BASE LINK                                                     -->
  <!-- ============================================================ -->
  <link name="base_link">
    <inertial>
      <origin rpy="0 0 0" xyz="-0.052335 0.089927 -0.002793"/>
      <mass value="5.0"/>
      <inertia ixx="0.01" ixy="0.0" ixz="0.0"
                           iyy="0.01" iyz="0.0"
                                       izz="0.01"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://cra_description/meshes/cr20/base_link.STL"/>
      </geometry>
      <material name="cr20_white"/>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://cra_description/meshes/cr20/base_link.STL"/>
      </geometry>
    </collision>
  </link>

  <joint name="world_joint" type="fixed">
    <origin rpy="0 0 0" xyz="0 0 0.03"/>
    <parent link="world"/>
    <child link="base_link"/>
  </joint>

  <!-- ============================================================ -->
  <!-- LINK 1                                                        -->
  <!-- ============================================================ -->
  <link name="Link1">
    <inertial>
      <origin rpy="0 0 0" xyz="-0.015458 -0.098638 1.041196"/>
      <mass value="8.0"/>
      <inertia ixx="0.05" ixy="0.0" ixz="0.0"
                           iyy="0.05" iyz="0.0"
                                       izz="0.02"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://cra_description/meshes/cr20/Link1.STL"/>
      </geometry>
      <material name="cr20_white"/>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://cra_description/meshes/cr20/Link1.STL"/>
      </geometry>
    </collision>
  </link>

  <joint name="joint1" type="revolute">
    <origin rpy="0 0 0" xyz="0 0 0.23"/>
    <parent link="base_link"/>
    <child link="Link1"/>
    <axis xyz="0 0 1"/>
    <limit effort="150.0" lower="-6.28" upper="6.28" velocity="3.14"/>
    <dynamics damping="10.0" friction="0.5"/>
  </joint>

  <!-- ============================================================ -->
  <!-- LINK 2                                                        -->
  <!-- ============================================================ -->
  <link name="Link2">
    <inertial>
      <origin rpy="0 0 0" xyz="-0.828899 0.0 0.115283"/>
      <mass value="6.0"/>
      <inertia ixx="0.1" ixy="0.0" ixz="0.0"
                          iyy="0.1" iyz="0.0"
                                     izz="0.02"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://cra_description/meshes/cr20/Link2.STL"/>
      </geometry>
      <material name="cr20_white"/>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://cra_description/meshes/cr20/Link2.STL"/>
      </geometry>
    </collision>
  </link>

  <joint name="joint2" type="revolute">
    <origin rpy="1.5708 1.5708 0" xyz="0 0 0"/>
    <parent link="Link1"/>
    <child link="Link2"/>
    <axis xyz="0 0 1"/>
    <limit effort="150.0" lower="-6.28" upper="6.28" velocity="3.14"/>
    <dynamics damping="10.0" friction="0.5"/>
  </joint>

  <!-- ============================================================ -->
  <!-- LINK 3                                                        -->
  <!-- ============================================================ -->
  <link name="Link3">
    <inertial>
      <origin rpy="0 0 0" xyz="-0.746 0.0 0.07573"/>
      <mass value="4.0"/>
      <inertia ixx="0.05" ixy="0.0" ixz="0.0"
                           iyy="0.05" iyz="0.0"
                                       izz="0.01"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://cra_description/meshes/cr20/Link3.STL"/>
      </geometry>
      <material name="cr20_white"/>
