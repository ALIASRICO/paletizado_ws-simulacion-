  </link>

  <joint name="joint4" type="revolute">
    <origin rpy="0 0 -1.5708" xyz="-0.746 0 0.1288"/>
    <parent link="Link3"/>
    <child link="Link4"/>
    <axis xyz="0 0 1"/>
    <limit effort="50.0" lower="-6.28" upper="6.28" velocity="3.14"/>
    <dynamics damping="5.0" friction="0.3"/>
  </joint>

  <!-- ============================================================ -->
  <!-- LINK 5                                                        -->
  <!-- ============================================================ -->
  <link name="Link5">
    <inertial>
      <origin rpy="0 0 0" xyz="0.0 0.07573 0.0"/>
      <mass value="2.0"/>
      <inertia ixx="0.005" ixy="0.0" ixz="0.0"
                            iyy="0.005" iyz="0.0"
                                         izz="0.003"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://cra_description/meshes/cr20/Link5.STL"/>
      </geometry>
      <material name="cr20_white"/>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://cra_description/meshes/cr20/Link5.STL"/>
      </geometry>
    </collision>
  </link>

  <joint name="joint5" type="revolute">
    <origin rpy="1.5708 0 0" xyz="0 -0.1288 0"/>
    <parent link="Link4"/>
    <child link="Link5"/>
    <axis xyz="0 0 1"/>
    <limit effort="50.0" lower="-6.28" upper="6.28" velocity="3.14"/>
    <dynamics damping="3.0" friction="0.2"/>
  </joint>

  <!-- ============================================================ -->
  <!-- LINK 6                                                        -->
  <!-- ============================================================ -->
  <link name="Link6">
    <inertial>
      <origin rpy="0 0 0" xyz="0.015458 0.658803 -0.213462"/>
      <mass value="1.0"/>
      <inertia ixx="0.002" ixy="0.0" ixz="0.0"
                            iyy="0.002" iyz="0.0"
                                         izz="0.001"/>
